{
  "name": "victory-standalone",
  "version": "0.0.1",
  "description": "",
  "main": "dist/victory-standalone.js",
  "scripts": {
    "build": "npm run build-lib  && npm run build-dist",
    "build-lib": "npm run clean-lib  && babel src -d lib --copy-files",
    "build-dist": "npm run clean-dist && npm run build-dist-min && npm run build-dist-dev",
    "build-dist-min": "webpack --bail --config ./config/webpack/webpack.config.js",
    "build-dist-dev": "webpack --bail --config ./config/webpack/webpack.config.dev.js",
    "clean": "npm run clean-lib && npm run clean-dist",
    "clean-lib": "rimraf lib",
    "clean-dist": "rimraf dist",
    "lint-source": "eslint --color --ext .js,.jsx src",
    "lint-config": "eslint --color --ext .js config",
    "lint-test": "eslint --color --ext .js,.jsx test",
    "lint": "concurrently --kill-others 'npm run lint-source' 'npm run lint-config' 'npm run lint-test'"
  },
  "author": "Matt Hink <matt.hink@formidable.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-lodash": "^3.2.8",
    "babel-plugin-syntax-jsx": "^6.13.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "concurrently": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^7.0.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-mocha": "^1.1.0",
    "eslint-plugin-react": "^3.6.2",
    "lodash-webpack-plugin": "^0.10.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "preact": "^6.0.2",
    "preact-compat": "^3.0.1",
    "victory": "^0.12.0"
  }
}
